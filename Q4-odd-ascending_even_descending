<body>
    <div>Output</div>
    <script>

        //QUE4: sort into ascending order
        let arr = [6,3,9,1,7,11,13,4]

        for(let i =0;i<arr.length;i++)
        {
            for (let j = i+1; j<arr.length; j++)
            {
                if(arr[i]>arr[j])
                {
                    let temp = arr[i];
                    arr[i] = arr[j];
                    arr[j] = temp;
                }
            }
            // console.log(arr);
        }

        console.log(arr);


    </script>
</body>
